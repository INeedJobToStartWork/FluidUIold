{
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "cursorOffset": -1,
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-packagejson", "prettier-plugin-sort-json"],
  "printWidth": 120,
  "proseWrap": "always",
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false
}
